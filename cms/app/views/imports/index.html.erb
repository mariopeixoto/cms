<div>
  <h1 class='table-title'>Imports</h1>
  <div class='table-actions'>
    <%= link_to 'New Import', new_import_path, class: 'btn btn-primary', role: 'button' %>
  </div>
</div>

<table class='table table-hover'>
  <thead>
    <tr>
      <th>#</th>
      <th>Debt report</th>
      <th>Status</th>
      <th>Created at</th>
    </tr>
  </thead>

  <tbody>
    <% @imports.each do |import| %>
      <tr>
        <td><%= import.id %></td>
        <td><%= import.debt_report.url %></td>
        <td><%= import.status %></td>
        <td><%= import.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
